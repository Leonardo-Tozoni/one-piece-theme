{
  "name": "one-piece-light",
  "type": "light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#0B5394"
    },
    "variable.constant": {
      "foreground": "#1a1a1a"
    },
    "variable.defaultLibrary": {
      "foreground": "#0B5394"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment, punctuation.definition.comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7A8B99"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#0D9488"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#9333EA"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#9333EA"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#0B5394"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class, entity.name.type.class",
      "settings": {
        "foreground": "#0B5394"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#0B5394"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#0B5394"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "foreground": "#0B5394"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#10B981"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D97706"
      }
    },
    {
      "name": "Markup: Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9333EA"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#0B5394"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7A8B99"
      }
    },
    {
      "name": "Markup: Code",
      "scope": "markup.inline.raw, markup.fenced_code.block",
      "settings": {
        "foreground": "#0D9488"
      }
    }
  ],
